@ARTICLE{Kharton20063695,
author={Kharton, V.V.a  b  and Shaula, A.L.a  and Snijkers, F.M.M.c  and Cooymans, J.F.C.c  and Luyten, J.J.c  and Marozau, I.P.a  and Viskup, A.P.b  and Marques, F.M.B.a  and Frade, J.R.a },
