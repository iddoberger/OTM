@ARTICLE{Balachandran1997409,
author={Balachandran, U.a  and Dusek, J.T.a  and Maiya, P.S.a  and Ma, B.a  and Mieville, R.L.a  and Kleefisch, M.S.b  and Udovich, C.A.b  and Fleisch, T.H.b  and Bose, A.C.c },
title={Development of dense ceramic membranes for methane conversion},
journal={Studies in Surface Science and Catalysis},
year={1997},
