@ARTICLE{Kamarudin200499,
author={Kamarudin, S.K. and Daud, W.R.W. and Som, A.Md. and Mohammad, A.W. and Takriff, S. and Masdar, M.S.},
title={The conceptual design of a PEMFC system via simulation},
journal={Chemical Engineering Journal},
year={2004},
